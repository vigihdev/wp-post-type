{
    "name": "vigihdev/wp-post-type",
    "description": "Advanced WordPress custom post type management with fluent API, dependency injection support, and comprehensive field management for modern WordPress development.",
    "keywords": [
        "wordpress",
        "post-type",
        "custom-post-type",
        "cpt",
        "wordpress-custom-post-type",
        "taxonomy",
        "meta-fields",
        "wordpress-development",
        "fluent-api",
        "dependency-injection",
        "vigihdev",
        "wp-post-type",
        "wordpress-library",
        "custom-fields",
        "wordpress-cms",
        "wordpress-framework",
        "cpt-manager",
        "wordpress-package",
        "composer-package",
        "wordpress-toolkit",
        "content-types",
        "wordpress-architecture"
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Vigih Dev",
            "email": "vigihdev@gmail.com",
            "role": "developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Vigihdev\\WpPostType\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vigihdev\\WpPostType\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html coverage",
        "dev": "php -S localhost:1292 dev.php"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "repo.thrubus.id"
        }
    ],
    "require": {
        "php": "^8.1",
        "vigihdev/wp-kernel": "dev-main",
        "vigihdev/serializer": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.x-dev"
    }
}